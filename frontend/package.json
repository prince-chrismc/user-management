{
  "name": "user-management-frontend",
  "version": "1.0.0-dev.1",
  "description": "An open-source UI delivering a responsive user management experience",
  "homepage": "https://github.com/prince-chrismc/user-management/tree/main/frontend#readme",
  "keywords": [
    "user",
    "management"
  ],
  "license": "MIT",
  "author": "prince-chrismc",
  "scripts": {
    "dev": "webpack serve --open --env env=dev --env API_URL=https://ec2-18-222-250-141.us-east-2.compute.amazonaws.com",
    "dev:prod": "yarn build --env API_URL=https://ec2-18-222-250-141.us-east-2.compute.amazonaws.com && serve dist/",
    "dev:bundleanalyzer": "yarn dev --env addons=bundleanalyzer",
    "build": "rimraf dist && cross-env NODE_ENV=production webpack --env env=prod",
    "build:bundleanalyzer": "yarn build --env addons=bundleanalyzer",
    "lint": "eslint --fix src/",
    "test": "cross-env NODE_ENV=dev API_URL=http://localhost:3001 jest",
    "coverage": "yarn test --coverage"
  },
